// -----------------------------------------------------------------------------
model User {
  id        String   @id @default(cuid())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  name      String
  username  String  @unique
  email     String  @unique(map: "User.email_unique")
  profileId String  @unique
  phone     String? @unique
  roleId    String

  // appearance
  newLook String?

  // stripe
  subscriptionId        String?
  returning             Boolean? @default(false)
  lastSubscriptionCheck String?
  customerId            String?  @unique
  plan                  String?

  // dealer
  omvicNumber String?
  dealer      String?
  position    String?
  dept        String?

  // activix
  activixActivated String?
  activixEmail     String?
  activisUserId    String?
  activixId        String?
  dealerAccountId  String?

  // microsoft
  microId          String?
  // name             String?
  givenName        String?
  familyName       String?
  identityProvider String?

  role          UserRole            @relation("UserRole", fields: [roleId], references: [id], onUpdate: Cascade)
  profile       UserProfile         @relation("UserProfile", fields: [profileId], references: [id], onUpdate: Cascade, onDelete: Cascade)
  // password      UserPassword?      @relation("UserPassword")
  images        Image[]             @relation("Images")
  notifications NotificationsUser[] @relation("NotificationsUser")
  notes         Note[]              @relation("UserNotes")
  //UserPassword  UserPassword?
  timeEntries   TimeEntry[]
  workorders    Workorder[]

  @@index([roleId])
  @@index([profileId])
}

